	url = {http://www.aimspress.com/rticle/doi/10.3934/mine.2021031},
	doi = {10.3934/mine.2021031},
	abstract = {We consider the so-called \textit{it ergodic} problem for weak solutions of elliptic equations in divergence form, complemented with Neumann boundary conditions. The simplest example reads as the following boundary value problem in a bounded domain of \${\textbackslash}mathbb\{R\}{\textasciicircum}N\$:

\$
			{\textbackslash}left{\textbackslash}\{\vphantom{\}} {\textbackslash}begin\{array\}\{l\}
 - \{{\textbackslash}rm\{div\}\}(A(x){\textbackslash}nabla u) + {\textbackslash}lambda = H(x,{\textbackslash}nabla u){\textbackslash};{\textbackslash};{\textbackslash};{\textbackslash};{\textbackslash};\{{\textbackslash}rm\{in\}\}{\textbackslash};{\textbackslash}Omega ,{\textbackslash}{\textbackslash}
A(x){\textbackslash}nabla u {\textbackslash}cdot {\textbackslash}vec n = 0{\textbackslash};{\textbackslash};{\textbackslash};{\textbackslash};{\textbackslash};{\textbackslash};{\textbackslash};\{{\textbackslash}rm\{on\}\}{\textbackslash};{\textbackslash}partial {\textbackslash}Omega ,
{\textbackslash}end\{array\} {\textbackslash}right.
			\$
 where \textit{A}(\textit{x}) is a coercive matrix with bounded coefficients, and \$H(x, {\textbackslash}nabla u)\$ has Lipschitz growth in the gradient and measurable \$x\$-dependence with suitable growth in some Lebesgue space (typically, \${\textbar}H(x, {\textbackslash}nabla u){\textbar}{\textbackslash}leq b(x) {\textbar}{\textbackslash}nabla u{\textbar}+ f(x)\$ for functions \textit{b}(\textit{x})∈ \textit{L}$^{\textrm{\textit{N}}}$(Ω) and \textit{f} (\textit{x}) ∈ \textit{L}$^{\textrm{\textit{m}}}$(Ω), \$m{\textbackslash}geq 1\$). We prove that there exists a unique real value \${\textbackslash}lambda\$ for which the problem is solvable in Sobolev spaces and the solution is unique up to addition of a constant. We also characterize the ergodic limit, say the singular limit obtained by adding a vanishing zero order term in the equation. Our results extend to weak solutions and to data in Lebesgue spaces \textit{L}$^{\textrm{\textit{N}}}$(Ω)  (or in the dual space (\textit{H}$^{\textrm{1}}$(Ω))'), previous results which were proved in the literature for bounded solutions and possibly classical or viscosity formulations.},
	language = {en},
	number = {4},
	urldate = {2021-05-07},
	journal = {Mathematics in Engineering},
	author = {Murat, François and Porretta, Alessio and Murat, François and Porretta, Alessio},
	year = {2021},
	note = {Number: mine-03-04-031},
	pages = {1--20},
}

