@Article{Concordel1996,
  author    = {Marie C. Concordel},
  title     = {Periodic Homogenization of Hamilton-Jacobi Equations: Additive Eigenvalues and Variational Formula},
  journal   = {Indiana University Mathematics Journal},
  year      = {1996},
  volume    = {45},
  number    = {4},
  pages     = {1095--1117},
  issn      = {00222518, 19435258},
  abstract  = {The classical first-order Hamilton-Jacobi equation $u^\epsilon _t+H(Du^\epsilon ,x/\epsilon )=0$ has a unique viscosity solution satisfying an initial condition uε = u0 when t = 0. When H is periodic in the second variable, it is known that uε converges as ε → 0 to the unique viscosity solution of the "averaged equation" ut + H̄(Du) = 0, with the same initial condition. In this paper, we assume that H is convex and we use optimal control theory to establish a relationship between the effective Hamiltonian H̄ and a nonlinear "additive" eigenvalue equation. For any convex Hamiltonian H, we prove existence and uniqueness of this additive eigenvalue, and we derive a variational formula for H̄.},
  publisher = {Indiana University Mathematics Department},
  url       = {http://www.jstor.org/stable/24899168},
}

