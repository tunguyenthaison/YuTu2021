  journal   = {Indiana University Mathematics Journal},
  year      = {2001},
  volume    = {50},
  number    = {3},
  pages     = {1113--1129},
  issn      = {00222518, 19435258},
  abstract  = {We consider the homogenization problem for fully nonlinear first order scalar partial differential equations
			 of {H}amilton--{J}acobi type such as
			 $u^\varepsilon (x)+H\left ( x,\frac{x}{\varepsilon },Du^\varepsilon (x) \right )=0, x\epsilon \mathbb{R}^N$,
			 where ε is a small positive parameter and H is a periodic function of the second variable. 
			 Our main results (Theorems 1.1 and 1.2 below)
			 give estimates on the rate of convergence of uε to the solution u of the homogenized problem
			 u(x) + H̄(x,Du(x)) = 0, x ∈ ℝN.},
  file      = {:References storage/Indexed/Caouzzo-Dolcetta and H. Ishii - On the Rate of Convergence in Homogenization of Hamilton-Jacobi Equations.pdf:PDF},
